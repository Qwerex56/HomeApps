{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DbConnectionConfig": {
    "HostName": "YourDbHost",
    "Port": "5432",
    "DbName": "YourDatabaseName",
    "Username": "YourDbUsername",
    "Password": "YourDbPassword"
  },
  "SeedUser": {
    "Name": "YourAdminName",
    "Email": "admin@example.com",
    "Password": "YourAdminPassword",
    "Role": "SystemOwner"
  },
  "JwtOptions": {
    "Issuer": "YourIssuer",
    "Audience": "YourAudience",
    "SecretKey": "YourSecretKey"
  },
  "CookieOptions": {
    "Domain": "YourDomain",
    "Secure": true,
    "HttpOnly": true,
    "SameSite": "None",
    "Path": "/"
  },
  "FrontendCorsOptions": {
    "FrontendDomain": "https://REPLACE_WITH_YOUR_FRONTEND_DOMAIN"
  }
}
