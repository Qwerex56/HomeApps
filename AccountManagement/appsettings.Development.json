{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DbConnectionConfig": {
    "HostName": "YourDbHost",
    "Port": "5432",
    "DbName": "YourDatabaseName",
    "Username": "YourDbUsername",
    "Password": "YourDbPassword"
  },
  "SeedUser": {
    "Name": "YourAdminName",
    "Email": "your.admin@email.com",
    "Password": "YourAdminPassword",
    "Role": "SystemOwner"
  },
  "JwtOptions": {
    "Issuer": "YourIssuer",
    "Audience": "YourAudience",
    "SecretKey": "YourSecretKey"
  }
}
